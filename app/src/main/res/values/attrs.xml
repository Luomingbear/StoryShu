<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleView">
        <attr name="title_string" format="string|reference" />
        <attr name="title_color" format="color|reference" />
        <attr name="title_size" format="dimension|reference" />
        <attr name="title_mode" format="enum">
            <enum name="MenuTitleSift" value="0" />
            <enum name="BackTitle" value="1" />
            <enum name="BackTitleGo" value="2" />
            <enum name="BackTitleShot" value="3" />
            <enum name="BackTitleSend" value="4" />
            <enum name="BackTitleMore" value="5" />
            <enum name="BackTitleImageGo" value="6" />
            <enum name="MenuTitleList" value="7" />
            <enum name="MenuTitleMap" value="8" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SelectorGroup">
        <attr name="selectorMode" format="enum">
            <enum name="CheckButton" value="0" />
            <enum name="RadioButton" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SelectorAdapterView">
        <attr name="selectorAdapterMode" format="enum">
            <enum name="CheckButton" value="0" />
            <enum name="RadioButton" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SelectorButton">
        <attr name="need_drawable" format="boolean" />
        <attr name="icon_padding" format="dimension|reference" />
        <attr name="layout_padding" format="dimension|reference" />
        <attr name="text_color" format="color|reference" />
        <attr name="text_string" format="string|reference" />
        <attr name="text_size" format="dimension|reference" />
        <attr name="icon_position" format="enum">
            <enum name="Left" value="0" />
            <enum name="Top" value="1" />
            <enum name="Right" value="2" />
            <enum name="Bottom" value="3" />
            <enum name="Center" value="4" />
        </attr>
    </declare-styleable>


    <declare-styleable name="RoundImageView">
        <attr name="Scale" format="float" />
        <attr name="BorderRadius" format="dimension|reference" />
        <attr name="RoundType">
            <enum name="circle" value="0" />
            <enum name="round" value="1" />
            <enum name="round_top_left_top_right" value="2" />
            <enum name="round_bottom_left_bottom_right" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AutoScaleLayout">
        <attr name="scaleRate" format="float" />
    </declare-styleable>

    <declare-styleable name="StoryEditView">
        <attr name="contentColor" format="color|reference" />
        <attr name="contentSize" format="dimension|reference" />
        <attr name="lineSpace" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="FontTextView">
        <attr name="textFont" format="enum">
            <enum name="HanYiZhongHeiJian" value="0" />
            <enum name="HanYiMeiHei" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SideSlipLayout">
        <attr name="homeLayout" format="reference" />
        <attr name="sideLayout" format="reference" />
    </declare-styleable>

    <declare-styleable name="BlurRelativeLayout">
        <attr name="blurDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="StorkTextView">
        <attr name="storkColor" format="color" />
    </declare-styleable>
</resources>